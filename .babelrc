{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8.9.3"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ],
  "ignore": [
    "node_modules/**"
  ]
}